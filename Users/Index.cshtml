@model List<PeopleSearchSite.Models.ApplicationUser>

@{
    Layout = "_Layout";
    ViewBag.Title = "Пользователи";
}

<h2>Пользователи</h2>

<p>
    <a href="/Users/Create">Добавить пользователя</a>
</p>

<table>
    <thead>
        <tr>
            <th>Email</th>
            <th>Логин</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.UserName</td>
            </tr>
        }
    </tbody>
</table>
